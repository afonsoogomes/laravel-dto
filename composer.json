{
    "name": "afonsoogomes/laravel-dto",
    "description": "A library for handling DTOs with Laravel.",
    "type": "library",
    "version": "1.0.1",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/console": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "AfonsoOGomes\\LaravelDTO\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "AfonsoOGomes\\LaravelDTO\\Providers\\DTOServiceProvider"
            ]
        }
    }
}
